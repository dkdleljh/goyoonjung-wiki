# Environment Variables for goyoonjung-wiki
# Copy this file to .env and fill in your actual values

# Discord Configuration
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN

# RSS Feed Configuration
RSS_URL=https://news.google.com/rss/search?q=고윤정+when:1d&hl=ko&gl=KR&ceid=KR:ko

# Agency Configuration
MAA_URL=https://maa.co.kr/artists/go-younjung

# Encyclopedia APIs
WIKIPEDIA_API_URL=https://ko.wikipedia.org/w/api.php?action=query&titles=%EA%B3%A0%EC%9C%A4%EC%A0%95&prop=revisions&rvprop=timestamp|user|comment&format=json
NAMUWIKI_URL=https://namu.wiki/w/%EA%B3%A0%EC%9C%A4%EC%A0%95

# Performance Settings
MAX_CONCURRENT_REQUESTS=6
REQUEST_TIMEOUT=25
CACHE_TTL=3600

# System Settings
TIMEZONE=Asia/Seoul
LOG_LEVEL=INFO
BACKUP_RETENTION_DAYS=30

# Security Settings
ENABLE_RATE_LIMITING=true
MAX_REQUESTS_PER_MINUTE=60
ENABLE_CORS=false

# AI/LLM Integration (Optional)
# OpenAI API for content generation
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_MODEL=gpt-4

# Anthropic Claude API for content generation
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key-here
ANTHROPIC_MODEL=claude-3-opus-20240229

# GitHub Integration for Backup to Releases
GITHUB_TOKEN=ghp_your_github_personal_access_token
GITHUB_REPO=yourusername/goyoonjung-wiki